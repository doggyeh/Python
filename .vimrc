"let GtagsCscope_Auto_Map = 1
"let GtagsCscope_Auto_Load = 1
"let GtagsCscope_Quiet = 1
"set cscopetag

set listchars=eol:$,tab:>-,trail:~,extends:>,precedes:<
set list
set nu
set hlsearch
set autoindent
set ruler
set showmode
set showmode
set showmatch
set background=dark
set backspace=2
set cursorline!
set tabstop=4
set shiftwidth=4
set softtabstop=4
set smarttab
set expandtab
syntax on
